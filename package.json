    "@babel/preset-react": "^7.18.6",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.34.0",
    "axios": "^1.3.4",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
